{
  "name": "gitmetrics",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!node_modules/**",
      "!coverage/**",
      "!jest.config.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js",
      "**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      ".*\\.disabled\\.js$"
    ]
  },
  "dependencies": {
    "axios": "^1.11.0",
    "minimist": "^1.2.0",
    "moment": "^2.24.0",
    "moment-business-time": "^0.7.1",
    "node-file-cache": "^1.0.2",
    "octokit": "^1.7.0"
  },
  "devDependencies": {
    "jest": "^30.0.5"
  }
}
